# Routes
# This file defines all application routes (Higher priority routes first)
# https://www.playframework.com/documentation/latest/ScalaRouting
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index
GET     /file                       controllers.HomeController.indexFile
GET     /mac-file                   controllers.MacController.indexFile

GET     /api/fetch1                 controllers.HomeController.fetch1
GET     /api/fetch2                 controllers.HomeController.fetch2
GET     /api/fetch3                 controllers.HomeController.fetch3
GET     /api/fetch4                 controllers.HomeController.fetch4
GET     /api/fetch5                 controllers.HomeController.fetch5
GET     /api/fetch6                 controllers.HomeController.fetch6
GET     /api/fetch7                 controllers.HomeController.fetch7
GET     /api/fetch8                 controllers.HomeController.fetch8
GET     /api/fetch9                 controllers.HomeController.fetch9
GET     /api/fetch10                controllers.HomeController.fetch10
GET     /api/fetch11                controllers.HomeController.fetch11

POST    /api/upload                 controllers.HomeController.upload
GET     /api/export                 controllers.MacController.export
GET     /api/export2                controllers.MacController.export2
GET     /api/export3                controllers.MacController.export3

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
